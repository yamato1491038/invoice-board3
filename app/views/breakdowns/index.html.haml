.wrapper
  .Title
    MY内訳書確認
  .Regular__form
    .Regular__form__content
      = form_with url: search_breakdowns_path, local: true, method: :get, class: "" do |f|

        = f.date_select :dating, {discard_day:true,use_month_numbers:true}, class: 'table_style'

        = f.submit "検索", class: "btn btn-info"
  .Regular__return
    .Regular__return__btn
      = link_to "戻る","/regulars",class: 'btn-sized btn btn-secondary'